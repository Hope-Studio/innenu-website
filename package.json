{
  "name": "innenu-website",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Mr.Hope",
  "scripts": {
    "analyze": "vue-cli-service build --mode analyze --modern",
    "build": "vue-cli-service build --modern",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "commit": "git cz",
    "inspect": "vue-cli-service inspect --modern > modern.js",
    "lint": "vue-cli-service lint --fix && prettier --check --write .",
    "lint:check": "vue-cli-service lint && prettier --check .",
    "normal:analyze": "vue-cli-service build --mode analyze",
    "normal:build": "vue-cli-service build",
    "normal:inspect": "vue-cli-service inspect > normal.js",
    "serve": "vue-cli-service serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "vue-cli-service lint",
    "*.{js,ts,json,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@ant-design/icons-vue": "5.1.5",
    "ant-design-vue": "^2.0.0-0",
    "axios": "0.20.0",
    "core-js": "3.6.5",
    "register-service-worker": "1.7.1",
    "screenfull": "5.0.2",
    "typescript": "4.0.3",
    "viewerjs": "1.6.2",
    "vue": "3.0.0",
    "vue-router": "^4.0.0-0",
    "vue-wechat-title": "2.0.7",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/mocha": "8.0.3",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "@vue/cli-plugin-babel": "4.5.6",
    "@vue/cli-plugin-e2e-cypress": "4.5.6",
    "@vue/cli-plugin-eslint": "4.5.6",
    "@vue/cli-plugin-pwa": "4.5.6",
    "@vue/cli-plugin-typescript": "4.5.6",
    "@vue/cli-plugin-unit-mocha": "4.5.6",
    "@vue/cli-service": "4.5.6",
    "@vue/compiler-sfc": "3.0.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "5.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "4.3.0",
    "less": "3.12.2",
    "less-loader": "7.0.1",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "sort-package-json": "1.46.0",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "vue-svg-loader": "^0.17.0-0",
    "webpack-bundle-analyzer": "3.9.0"
  }
}
